@{
    Bundles.Reference("Scripts/jquery-1.9.min.js");
    Bundles.Reference("Scripts/lazy-1.8.4.min.js");
    Bundles.Reference("Scripts/modernizr-1.7.min.js");
    Bundles.Reference("Scripts/bootstrap.min.js");
    Bundles.Reference("Scripts/storage.js");
    Bundles.Reference("Scripts/knockout-2.1.0.min.js");
    Bundles.Reference("Scripts/toastr.js");
    Bundles.Reference("Scripts/userlogin.js");
    Bundles.Reference("Scripts/startup.js");

    Bundles.Reference("//js.live.net/v5.0/wl.js");
    Bundles.Reference("//cdnjs.cloudflare.com/ajax/libs/dropbox.js/0.9.1/dropbox.min.js");
    //Bundles.Reference("https://www.dropbox.com/static/api/1/dropbox.js");
    Bundles.Reference("Scripts/Drives/skydrive.js");
    Bundles.Reference("Scripts/Drives/dbdrive.js");




    Bundles.Reference("/Content/bootstrap.min.css");
    Bundles.Reference("/Content/Site.css");
    Bundles.Reference("/Content/toastr.css");
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>online3D</title>
 
     @Bundles.RenderStylesheets()

</head>
<body class="gridbody" id="body">     

        <div id="loginDiv" style="float:right;padding:20px">
           <button id="access" class="btn btn-large"  type="button" > Sign in</button>
         
        </div>

        <div class="hero-unit gridbody" id="header" >
           <h1>Online3D</h1>
           <p> View, Share and Collaborate 3D with models online</p>       
           <img id="iconImageSrc" class="introIcon" src="@Url.Content("~/Content/Images/logo.png")" data-original="@Url.Content("~/Content/Images/3D-icon.png")"/>                  
        </div>

        <div class="container" id="fileDropDiv">    
                     
            <span id="intro_text">Drop 3D model here, or</span> 
            <input type="file" id="fileLoader" name="files" title="Load File" style="visibility: hidden;
                position: absolute; top: -50px; left: -50px" />
      
           <div class="btn-group" id="selectfile_button">
                <button class="btn btn-large  btn-primary" style="font-size: 23px;" data-toggle="dropdown" href="#">
                    Open file
                <span class="caret"></span>
                </button>
                <ul class="dropdown-menu">
                    <li> <a href="#" onclick="startup.openFileDialog();">Computer</a> </li>
                    <li> <a href="#"  onclick="startup.openDropboxDrive();">Dropbox (ALFA)</a></li>
                    <li> <a href="#"  onclick="startup.openSkyDrive();">Sky Drive (ALFA)</a></li>
                   @* <li> <a href="#"  onclick="startup.openGoogleDrive();">Google Drive</a></li>*@                   
                </ul>
                </div>
            </div>

        <div id="db_progress"  class="progress progress-striped active" style="margin: 30px auto auto auto; width:300px; height: 32px; visibility:hidden; z-index: 1;
position: relative;">
           <div class="bar" style="width: 100%;"></div>
        </div>

        <div id="introimage">           
           <img id="introImageSrc" src="@Url.Content("~/Content/Images/logo.png")" data-original="@Url.Content("~/Content/Images/intro.png")"/>               
        </div>   

        <div id="sample_preview" style="padding: 33px;position: absolute;top: 420px;visibility:hidden">
             <div class="span4" style="padding: 5px 5px 2px 5px;">    
                                            
                 <a data-bind="attr:{href:ID}" id="link">                
                    <img class="preview_image" style="max-width: 100%;max-height: 100%;" data-bind="attr:{src:ModelImage}"></img>
                 </a>
                  <span />
                  <p id="fileName" data-bind="text:SessionName"></p>    
             </div>
        </div>
      
        @Bundles.RenderScripts()
        <script type="text/javascript" async src="https://www.dropbox.com/static/api/1/dropbox.js" id="dropboxjs" data-app-key="li6cyy4xgngfash"></script>
    
</body>

</html>
