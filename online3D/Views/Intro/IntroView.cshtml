<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>@* <link href="@Url.Content("~/Content/social.css")" rel="stylesheet" type="text/css" />*@
    <link rel="stylesheet/less" type="text/css" href="@Url.Content("~/Content/styles.less")">
    <link href="@Url.Content("~/Content/toastr.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/less-1.3.0.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/storage.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/bootstrap.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/toastr.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/userlogin.js")" type="text/javascript"></script>


</head>
<body>
   
   
    <div id="main">
        <div class="drop_zone" id="fileDropDiv">
        
            <span id="intro_text">Drop 3D model here, or</span> 
            <input type="file" id="fileLoader" name="files" title="Load File" style="visibility: hidden;
                position: absolute; top: -50; left: -50" />
            <div  id="db_progress" class="progress-bar blue stripes" style="visibility: hidden;">
                <span style="width: 100%;"></span>
            </div>
            <a href="#" id="selectfile_button" onclick="openFileDialog();">Load from disk</a>
        </div>

      
    </div>
    <div id="footer">
        <ul id="shared" style="display: inline; margin-right: -85%">
            <li style="display: inline">
                <img class="sharebuttons" src="@Url.Content("~/Content/Images/facebookb.png")" width="20" height="20"/><a
                    href="#" title="Share on Facebook"></a></li>
            <li style="display: inline">
                <img class="sharebuttons" src="@Url.Content("~/Content/Images/google-plus.png")" width="20" height="20"/><a
                    href="#" title="Share on Google"></a></li>
            <li style="display: inline">
                <img class="sharebuttons" src="@Url.Content("~/Content/Images/bloggero.png")" width="20" height="20"/><a
                    href="#" title="Share on Blogger"></a></li>
            <li style="display: inline">
                <img class="sharebuttons" src="@Url.Content("~/Content/Images/email.png")" width="20" height="20"/><a
                    href="#" title="Share via e-Mail"></a></li>
            <li style="display: inline">
                <img class="sharebuttons" src="@Url.Content("~/Content/Images/linkedin.png")" width="20" height="20"/><a
                    href="#" title="Share on LinkedIn"></a></li>
            <li style="display: inline">
                <img class="sharebuttons" src="@Url.Content("~/Content/Images/stackoverflow.png")" width="20" height="20"/><a
                    href="#" title="Share on Stack Overflow"></a></li>
        </ul>
    </div>
  
</body>
</html>
