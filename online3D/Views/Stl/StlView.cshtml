<div class="menubox">		
	<ul id="ldd_menu" class="ldd_menu">
		<li id="access_menu">
			<span>Access</span>
			<div class="ldd_submenu">
				<ul id="access_ul" data-bind="foreach: access_menu">
					<li class="ldd_heading"><a data-bind="attr:{id:id}, text:text, click: function(e) { callback(); }, visible: itemVisible" href="#" ></a></li>
				</ul>				
			</div>
		</li>
		<li id="save_menu">
			<span>Save</span>
			<div class="ldd_submenu">
				<ul id="save_ul" data-bind="foreach: save_menu">
					<li class="ldd_heading"><a data-bind="attr:{id:id}, text:text, click: function(e) { callback(); }" ></a></li>
				</ul>
				
			</div>
		</li>
        <li id="view_menu">
			<span>View</span>
			<div class="ldd_submenu">
				<ul id="view_ul" data-bind="foreach: view_menu">
                    <li class="ldd_heading"><a data-bind="attr:{id:id}, text:text, click: function(e) { callback(); }" ></a></li>                   			
				</ul>
				
			</div>
		</li>
		
	</ul>

     <div class="logo">
        <img src="@Url.Content("~/Content/Images/logo.png")"  />
    </div>
</div>


@*hiddent field for holding the model(s) unique ID that has to be loaded *@
@if ((@ViewBag.ID) != null) {
    <p id="bag" style="visibility:hidden">@(ViewBag.ID)</p>
}


<div id="3DArea"style="margin:-50px">
  
</div>

<div id="basicPanel" class="basicPanel">
    
    <div id="innerbasicPanel" data-bind="foreach: navigation">
        <a class="toolboxbotton" data-bind="click: function(e) { callback(); }, 
                            event: { mouseover: 
                                                function(e) 
                                                { 
                                                   return false;
                                                } 
                                    }, attr:{alt:alt}">
            <img class="imageOnButton" data-bind="attr:{id:id, src:src}" />
        </a>
    </div>
</div>

<div id="toolsPanel" > 
    <div id="tools" data-bind="foreach: tools">
        <a class="toolboxbotton" data-bind="click: function(e) { callback(); }, 
                            event: { mouseover: 
                                                function(e) 
                                                {                                                    
                                                   return false;
                                                } 
                                    }, attr:{alt:alt}">
            <img class="imageOnButton" data-bind="attr:{id:id, src:src}" />
        </a>
    </div>
</div>


<div id="fileInfo" class="fileInfo" >
    <div class="accordion"  id="infoList"data-bind="foreach: {data : modelsInformation}">
        <div class="accordion-group" >
            <div class="accordion-heading" >
                <div class="accordion-toggle" data-toggle="collapse" data-parent="#infoList" href="#collapseOne"
                        data-bind="style:{ 'backgroundColor': isVisible() === true ? 'transparent' : 'rgba(250, 34, 86, 0.45)' }">
                    <span id="index" class="badge" data-bind="text:$index, click:click.bind($data), style:{ 'background-color': color}">
                     </span>
                    <span class="label label-info" data-bind="text:fileName"></span>
                </div>
            </div>
            <div id="collapseOne" class="accordion-body collapse in">
                <div class="accordion-inner">
                   <span class="label label-inverse" data-bind="text: fileSize"></span>
                   <span class="label label-inverse" data-bind="text: faceCount"></span>
                   <span class="label label-inverse" data-bind="text: vertexCount"></span>
                </div>
            </div>
        </div>       
    </div>
</div>


